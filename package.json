{
  "name": "string-manager",
  "version": "3.0.0",
  "description": "npm dependency to manage your string",
  "main": "dist/index.js",
  "type": "module",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "babel src -d dist",
    "prod:build": "NODE_ENV=production babel src -d dist",
    "test": "mocha --compilers js:babel-core/register",
    "test:watch": "mocha --watch ./test --compilers js:babel-core/register",
    "release:beta": "standard-version -p beta",
    "release": "standard-version && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yussan/string-manager-npm.git"
  },
  "keywords": [
    "npm",
    "string",
    "manager",
    "string",
    "helper"
  ],
  "author": "yussan <https://twitter.com/xyussanx>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "standard-version": "^8.0.1"
  },
  "dependencies": {}
}
